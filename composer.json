{
    "name": "dricle/laravel-shopping-cart",
    "description": "Laravel Shopping cart",
    "keywords": ["laravel", "shopping cart", "cart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dricle",
            "email": "dricle.llp@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/validation": "^10.0|^11.0",
        "illuminate/translation": "^10.0|^11.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "Dricle\\": "src/Dricle"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Dricle\\Cart\\CartServiceProvider"
            ],
            "aliases": {
                "Cart": "Dricle\\Cart\\Facades\\CartFacade"
            }
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dricle\\Tests\\": "tests/"
        }
    }
}
